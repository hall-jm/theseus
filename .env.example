THESEUS_APP_RELEASE=0
THESEUS_APP_MAJOR=0
THESEUS_APP_MINOR=1
THESEUS_APP_VERSION=${THESEUS_APP_RELEASE}.${THESEUS_APP_MAJOR}.${THESEUS_APP_MINOR}
THESEUS_APP_VERSION_STRING=theseus-cli/${THESEUS_APP_VERSION}

## Schema Versioning
THESEUS_LOG_SCHEMA_RELEASE=0
THESEUS_LOG_SCHEMA_MAJOR=0
